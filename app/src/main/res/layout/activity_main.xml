<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context=".MainActivity">


    <androidx.viewpager.widget.ViewPager
        android:id="@+id/viewPager"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="32dp"
        android:layout_marginTop="150dp"
        android:layout_marginEnd="32dp"
        android:layout_marginBottom="400dp"
        android:background="#00FFFFFF"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:layout_constraintVertical_bias="0.036">

    </androidx.viewpager.widget.ViewPager>

    <com.google.android.material.tabs.TabLayout
        android:id="@+id/tab_layout"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="400dp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:tabBackground="@drawable/tab_selector"
        app:tabGravity="center"
        app:tabIndicatorHeight="0dp" />

    <ImageButton
        android:id="@+id/gpsButton"
        android:layout_width="132dp"
        android:layout_height="116dp"
        android:layout_marginBottom="252dp"
        android:background="#00FFFFFF"
        android:contentDescription="@string/gpsbutton"
        android:onClick="GPSbutton"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.138"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@mipmap/gps_btn" />

    <ImageButton
        android:id="@+id/climbButton"
        android:layout_width="132dp"
        android:layout_height="116dp"
        android:layout_marginBottom="252dp"
        android:background="#00FFFFFF"
        android:contentDescription="@string/Climbbtn"
        android:onClick="Climbbutton"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.827"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@mipmap/hiking_btn" />


    <ImageButton
        android:id="@+id/picnicButton"
        android:layout_width="132dp"
        android:layout_height="116dp"
        android:layout_marginBottom="68dp"
        android:background="#00FFFFFF"
        android:contentDescription="@string/Picnicbtn"
        android:onClick="Picnicbutton"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.138"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@mipmap/picnic_btn" />

    <ImageButton
        android:id="@+id/restaurantButton"
        android:layout_width="132dp"
        android:layout_height="116dp"
        android:layout_marginBottom="56dp"
        android:background="#00FFFFFF"
        android:contentDescription="@string/Restaurantbtn"
        android:onClick="Restaurantbutton"
        android:scaleType="fitCenter"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.827"
        app:layout_constraintStart_toStartOf="parent"
        app:srcCompat="@mipmap/rst_btn" />

    <ImageView
        android:id="@+id/imageView5"
        android:layout_width="100dp"
        android:layout_height="78dp"
        android:layout_marginBottom="140dp"
        android:background="#00FFFFFF"
        android:contentDescription="@string/Climbbtn"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.165"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/gpsButton"
        app:layout_constraintVertical_bias="0.0"
        app:srcCompat="@drawable/gps_btn_desc" />

    <ImageView
        android:id="@+id/imageView6"
        android:layout_width="144dp"
        android:layout_height="72dp"
        android:contentDescription="@string/Picnicbtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.142"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/picnicButton"
        app:srcCompat="@drawable/picnic_btn_desc" />

    <ImageView
        android:id="@+id/imageView8"
        android:layout_width="212dp"
        android:layout_height="47dp"
        android:contentDescription="@string/Restaurantbtn"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.915"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/restaurantButton"
        app:srcCompat="@drawable/rst_btn_desc" />

    <ImageView
        android:id="@+id/banner_app_name"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="140dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="140dp"
        android:layout_marginBottom="850dp"
        android:contentDescription="@string/main_activity_view_app_name"
        android:scaleType="fitCenter"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/main_top_banner" />

    <ImageButton
        android:id="@+id/side_menu_btn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="10dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="60dp"
        android:layout_marginBottom="850dp"
        android:background="#00FFFFFF"
        android:contentDescription="@string/main_side_menu_button"
        android:scaleType="fitCenter"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toStartOf="@+id/banner_app_name"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/side_menu" />

    <ImageButton
        android:id="@+id/main_search_btn"
        android:layout_width="0dp"
        android:layout_height="0dp"
        android:layout_marginStart="60dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="10dp"
        android:layout_marginBottom="850dp"
        android:background="#00FFFFFF"
        android:contentDescription="@string/main_search_button"
        android:scaleType="fitCenter"
        android:visibility="visible"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/banner_app_name"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/search" />


</androidx.constraintlayout.widget.ConstraintLayout>